<table class="table table-bordered table-striped table-responsive">
  <thead>
    <tr>
      <th>Event</th>
      <th>Start Date</th>
      <th>End Date</th>
      <th>Color Code</th>
    </tr>
  </thead>
  <tbody>
    {{#each}}
      <tr>
        <td>{{event}}</td>
        <td>{{simple-date startDate}}</td>
        <td>{{simple-date endDate}}</td>
        <td>{{colorCode}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>

{{outlet}}
